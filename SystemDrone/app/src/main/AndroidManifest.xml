<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.systemdrone">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="SmartDrone"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Setting"/>
        <activity android:name=".UserMode"/>
        <activity android:name=".SignUp"/>
        <activity android:name=".Logout"/>
        <activity android:name=".Farming"/>
        <activity android:name=".AgriculturalLand"/>
        <activity android:name=".RealTimeMonitoring"/>
        <activity android:name=".FarmImage"/>
        <activity android:name=".AlarmSimulation"/>
        <activity android:name=".Alarm"/>
    </application>

</manifest>